package org.yudin.ram.system;

public class Reader {
	private static int array[];
	private static int curr;

	public static int readNext() {
		int result = array[curr];
		curr = curr+1;
		return result;
		
	}

	public int[] getArray() {
		return array;
	}

	public void setArray(int array[]) {
		this.array = array;
	}
}
