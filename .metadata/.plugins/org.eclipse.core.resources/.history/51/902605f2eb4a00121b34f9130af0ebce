package org.yudin.ram.inter;

public class Commands {
	
	// MEMORY OPERATIONS
	
	public final int READ = 0;
	public final int LOAD = 1;
	public final int STORE = 2;
	public final int WRITE = 3;
	
	// MATH OPERATIONS
	
	public final int ADD = 4;
	public final int DIV = 5;
	public final int SUBT = 6;
	public final int MULT = 7;
	
	//JUMPERS
	
	public final int JUMP = 8;
	public final int JZERO = 9;
	public final int JGTZ = 10;
	public final int JPOS = 11;
	public final int JNEG = 12;
	
	//ADMIN
	
	public final int HALT = 13;
	public final int REST = 14;
	
}
